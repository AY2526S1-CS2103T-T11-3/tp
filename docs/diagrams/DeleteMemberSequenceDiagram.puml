@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":ClubBookParser" as ClubBookParser LOGIC_COLOR
participant ":DeleteMemberCommandParser" as DeleteMemberCommandParser LOGIC_COLOR
participant "d:DeleteMemberCommand" as DeleteMemberCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("deleteMember 1")
activate LogicManager

LogicManager -> ClubBookParser : parseCommand("deleteMember 1")
activate ClubBookParser

create DeleteMemberCommandParser
ClubBookParser -> DeleteMemberCommandParser
activate DeleteMemberCommandParser

DeleteMemberCommandParser --> ClubBookParser
deactivate DeleteMemberCommandParser

ClubBookParser -> DeleteMemberCommandParser : parse("1")
activate DeleteMemberCommandParser

create DeleteMemberCommand
DeleteMemberCommandParser -> DeleteMemberCommand
activate DeleteMemberCommand

DeleteMemberCommand --> DeleteMemberCommandParser :
deactivate DeleteMemberCommand

DeleteMemberCommandParser --> ClubBookParser : d
deactivate DeleteMemberCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
DeleteMemberCommandParser -[hidden]-> ClubBookParser
destroy DeleteMemberCommandParser

ClubBookParser --> LogicManager : d
deactivate ClubBookParser

LogicManager -> DeleteMemberCommand : execute(m)
activate DeleteMemberCommand

DeleteMemberCommand -> Model : deleteMember(1)
activate Model

Model --> DeleteMemberCommand
deactivate

create CommandResult
DeleteMemberCommand -> CommandResult
activate CommandResult

CommandResult --> DeleteMemberCommand
deactivate CommandResult

DeleteMemberCommand --> LogicManager : r
deactivate DeleteMemberCommand

[<--LogicManager
deactivate LogicManager
@enduml
